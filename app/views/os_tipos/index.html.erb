<p id="notice"><%= notice %></p>

<h1>Listando Os Tipos</h1>

<table class="table table-bordered table-hover table-condensed">

  <thead>
    <tr>
      <th>Os tipo</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @os_tipos.each do |os_tipo| %>
      <tr>
        <td><%= os_tipo.nome %></td>
        <td><%= link_to '', os_tipo,class: "glyphicon glyphicon-zoom-in" %></td>
        <td><%= link_to '', edit_os_tipo_path(os_tipo),class: "glyphicon glyphicon-edit" %></td>
        <td><%= link_to '', os_tipo, method: :delete, data: { confirm: 'VocÃª tem certeza?' },class: "glyphicon glyphicon-remove" %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'Novo Os tipo', new_os_tipo_path %>
