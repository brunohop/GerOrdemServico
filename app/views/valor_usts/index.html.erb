<p id="notice"><%= notice %></p>

<h1>Listando Valor de UST</h1>

<table class="table table-bordered table-hover table-condensed">
  <thead>
    <tr>
      <th>Valor</th>
      <th>Inicio vigência</th>
      <th>Fim vigência</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @valor_usts.each do |valor_ust| %>
      <tr>
        <td><%= valor_ust.valor %></td>
        <td><%= valor_ust.inicioVigencia %></td>
        <td><%= valor_ust.fimVigencia %></td>
        <td><%= link_to 'Exibir', valor_ust %></td>
        <td><%= link_to 'Editar', edit_valor_ust_path(valor_ust) %></td>
        <td><%= link_to 'Deletar', valor_ust, method: :delete, data: { confirm: 'Você tem certeza?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'Novo Valor de UST', new_valor_ust_path %>
