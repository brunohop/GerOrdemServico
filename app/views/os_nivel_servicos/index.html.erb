<p id="notice"><%= notice %></p>

<h1>Listando Níveis de Serviço das Ordens de Serviço</h1>

<table class="table table-bordered table-hover table-condensed">

  <thead>
    <tr>
      <th>Os</th>
      <th>Nivel servico</th>
      <th>Indicador</th>
      <th>Pontos</th>
      <th>Valor</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @os_nivel_servicos.each do |os_nivel_servico| %>
      <tr>
        <td><%= os_nivel_servico.os_id %></td>
        <td><%= os_nivel_servico.nivel_servico %></td>
        <td><%= os_nivel_servico.indicador %></td>
        <td><%= os_nivel_servico.pontos %></td>
        <td><%= os_nivel_servico.valor %></td>
        <td><%= link_to '', os_nivel_servico,class: "glyphicon glyphicon-zoom-in" %></td>
        <td><%= link_to '', edit_os_nivel_servico_path(os_nivel_servico),class: "glyphicon glyphicon-edit" %></td>
        <td><%= link_to '', os_nivel_servico, method: :delete, data: { confirm: 'Você tem certeza?' },class: "glyphicon glyphicon-remove" %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'Novo Os nivel servico', new_os_nivel_servico_path %>
