<p id="notice"><%= notice %></p>

<h1>Listando Níveis de Serviço da Ordem de Serviço</h1>

<table class="table table-bordered table-hover table-condensed">

  <thead>
    <tr>
      <th>Os</th>
      <th>Nivel servico</th>
      <th>Indicador</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @ordem_servico.os_nivel_servicos.each do |os_nivel_servico| %>
      <tr>
        <td><%= os_nivel_servico.os_id %></td>
        <td><%= os_nivel_servico.nivel_servico %></td>
        <td><%= os_nivel_servico.indicador %></td>
        <td><%= link_to 'Exibir', os_nivel_servico %></td>
        <td><%= link_to 'Editar', edit_os_nivel_servico_path(os_nivel_servico) %></td>
        <td><%= link_to 'Excluir', os_nivel_servico, method: :delete, data: { confirm: 'Você tem certeza?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
