<%= simple_form_for(@tarefa_deb_tecnico) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :tare_id, :as => :hidden  %>
    <%= f.input :debt_id,:collection => DebitoTecnico.all,label_method: :nome,:label => "Débito Técnico" ,:include_blank => false %>

  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
