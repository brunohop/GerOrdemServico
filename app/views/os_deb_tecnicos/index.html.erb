<p id="notice"><%= notice %></p>

<h1>Listando Débitos Técnicos das Ordens de Serviço</h1>

<table class="table table-bordered table-hover table-condensed">

  <thead>
    <tr>
      <th>Débito Técnico</th>
      <th>Ordem de Serviço</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @os_deb_tecnicos.each do |os_deb_tecnico| %>
      <tr>
        <td><%= os_deb_tecnico.debito_tecnico.nome%></td>
        <td><%= os_deb_tecnico.ordem_servico.nome %></td>
        <td><%= link_to '', os_deb_tecnico,class: "glyphicon glyphicon-zoom-in" %></td>
        <td><%= link_to '', edit_os_deb_tecnico_path(os_deb_tecnico),class: "glyphicon glyphicon-edit" %></td>
        <td><%= link_to '', os_deb_tecnico, method: :delete, data: { confirm: 'Você tem certeza?' },class: "glyphicon glyphicon-remove" %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'Novo Os deb tecnico', new_os_deb_tecnico_path %>
