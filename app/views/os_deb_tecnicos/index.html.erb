<p id="notice"><%= notice %></p>

<h1>Listando Os Deb Tecnicos</h1>

<table class="table table-bordered table-hover table-condensed">

  <thead>
    <tr>
      <th>Debt</th>
      <th>Os</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @os_deb_tecnicos.each do |os_deb_tecnico| %>
      <tr>
        <td><%= os_deb_tecnico.debt_id %></td>
        <td><%= os_deb_tecnico.os_id %></td>
        <td><%= link_to 'Exibir', os_deb_tecnico %></td>
        <td><%= link_to 'Editar', edit_os_deb_tecnico_path(os_deb_tecnico) %></td>
        <td><%= link_to 'Excluir', os_deb_tecnico, method: :delete, data: { confirm: 'VocÃª tem certeza?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

