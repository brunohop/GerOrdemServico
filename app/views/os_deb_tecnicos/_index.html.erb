<p id="notice"><%= notice %></p>

<h1>Listando Débitos Técnicos da Ordem de Serviço</h1>

<table class="table table-bordered table-hover table-condensed">

  <thead>
    <tr>
      <th>Débito Técnico</th>
      <th>Ordem de Serviço</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @ordem_servico.os_deb_tecnicos.each do |os_deb_tecnico| %>
      <tr>
        <td><%= os_deb_tecnico.debito_tecnico.nome%></td>
        <td><%= os_deb_tecnico.ordem_servico.nome %></td>
        <td><%= link_to 'Exibir', os_deb_tecnico %></td>
        <td><%= link_to 'Editar', edit_os_deb_tecnico_path(os_deb_tecnico) %></td>
        <td><%= link_to 'Excluir', os_deb_tecnico, method: :delete, data: { confirm: 'Você tem certeza?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
