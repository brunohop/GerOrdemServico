<p id="notice"><%= notice %></p>

<h1>Listando Debito Tecnicos</h1>

<table class="table table-bordered table-hover table-condensed">

  <thead>
    <tr>
      <th>Debito tecnico</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @debito_tecnicos.each do |debito_tecnico| %>
      <tr>
        <td><%= debito_tecnico.nome %></td>
        <td><%= link_to '', debito_tecnico,class: "glyphicon glyphicon-zoom-in" %></td>
        <td><%= link_to '', edit_debito_tecnico_path(debito_tecnico),class: "glyphicon glyphicon-edit" %></td>
        <td><%= link_to '', debito_tecnico, method: :delete, data: { confirm: 'VocÃª tem certeza?' },class: "glyphicon glyphicon-remove" %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'Novo Debito tecnico', new_debito_tecnico_path %>
