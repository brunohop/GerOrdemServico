<%= simple_form_for(@entregavel) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :nome %>
    <%= f.input :perfil %>
    <%= f.input :ativo, as: :radio_buttons, , :as => :hidden %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
