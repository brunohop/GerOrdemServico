<p id="notice"><%= notice %></p>

<h1>Listando Entregavels</h1>

<table class="table table-bordered table-hover table-condensed">

  <thead>
    <tr>
      <th>Entregavel</th>
      <th>Ativo</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @entregavels.each do |entregavel| %>
      <tr>
        <td><%= entregavel.nome %></td>
        <td><%= entregavel.ativo %></td>
        <td><%= link_to 'Exibir', entregavel %></td>
        <td><%= link_to 'Editar', edit_entregavel_path(entregavel) %></td>
        <td><%= link_to 'Excluir', entregavel, method: :delete, data: { confirm: 'VocÃª tem certeza?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

