<p id="notice"><%= notice %></p>

<h1>Listando Os Tarefas</h1>

<table class="table table-bordered table-hover table-condensed">

  <thead>
    <tr>
      <th>Catl</th>
      <th>Prod</th>
      <th>Pess abertura tarefa</th>
      <th>Tarefa atribuida</th>
      <th>Entregavel</th>
      <th>Os</th>
      <th>Id tarefa redmnine</th>
      <th>Ust tarefa</th>
      <th>Nurm horastarefa</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @os_tarefas.each do |os_tarefa| %>
      <tr>
        <td><%= os_tarefa.catl_id %></td>
        <td><%= os_tarefa.prod_id %></td>
        <td><%= os_tarefa.pess_abertura_tarefa_id %></td>
        <td><%= os_tarefa.pess_tarefa_atribuida_id %></td>
        <td><%= os_tarefa.entregavel_id %></td>
        <td><%= os_tarefa.os_id %></td>
        <td><%= os_tarefa.id_tarefa_redmine %></td>
        <td><%= os_tarefa.ust_tarefa %></td>
        <td><%= os_tarefa.horastarefa %></td>
        <td><%= link_to 'Exibir', os_tarefa %></td>
        <td><%= link_to 'Editar', edit_os_tarefa_path(os_tarefa) %></td>
        <td><%= link_to 'Excluir', os_tarefa, method: :delete, data: { confirm: 'VocÃª tem certeza?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

