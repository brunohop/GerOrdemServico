<p id="notice"><%= notice %></p>

<h1>Listando Projetos</h1>

<table class="table table-bordered table-hover table-condensed">

  <thead>
    <tr>
      <th>Projeto</th>
      <th>Id proj redmine</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @projetos.each do |projeto| %>
      <tr>
        <td><%= projeto.nome %></td>
        <td><%= projeto.id_proj_redmine %></td>
        <td><%= link_to '', projeto,class: "glyphicon glyphicon-zoom-in" %></td>
        <td><%= link_to '', edit_projeto_path(projeto),class: "glyphicon glyphicon-edit" %></td>
        <td><%= link_to '', projeto, method: :delete, data: { confirm: 'VocÃª tem certeza?' },class: "glyphicon glyphicon-remove" %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

